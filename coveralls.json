{
  "skip_files": [
    "^test/.*",
    "lib/durandal/migrations/*",
    "lib/durandal/helpers/fake_data/*"
  ],
  
  "terminal_options": {
    "file_column_width": 60
  },
  
  "coverage_options": {
    "treat_no_relevant_lines_as_covered": true,
    "output_dir": "cover/",
    "minimum_coverage": 90
  }
}